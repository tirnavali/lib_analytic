<div class="">
  <div class="wrapper-div col-16 p-3">    
    <p>
      <%=form.label :pub_type, t(:pub_type).titleize %>
      <%=form.collection_select(:pub_type_id, PubType.all, :id, :to_s, {}, {class: "form-control form-control-sm"}) %>    
    </p>
    <p>
      <%= form.label :pub_arrived_as_supply, t(:pub_arrived_as_supply).titleize %>
      <%= form.number_field :pub_arrived_as_supply , class: "form-control"%>
    </p>
    <p>
      <%= form.label :pub_arrived_as_gift, t(:pub_arrived_as_gift).titleize%>
      <%= form.number_field :pub_arrived_as_gift , class: "form-control"%>
    </p>  
    <p>
      <%= form.label :pub_bought, t(:pub_bought).titleize %>
      <%= form.number_field :pub_bought , class: "form-control"%>
    </p> 
    <p>
      <%= form.label :pub_saved_as_supply, t(:pub_saved_as_supply).titleize %>
      <%= form.number_field :pub_saved_as_supply , class: "form-control"%>
    </p> 
    <p>
      <%= form.label :pub_saved_as_gift, t(:pub_saved_as_gift).titleize %>
      <%= form.number_field :pub_saved_as_gift , class: "form-control"%>
    </p> 
    <p>
      <%= form.label :pub_saved_as_old, t(:pub_saved_as_old).titleize %>
      <%= form.number_field :pub_saved_as_old , class: "form-control"%>
    </p> 
    <p>
       <%= link_to_remove_nested(form, fields_wrapper_selector: '.wrapper-div', link_text: t(:remove).titleize, link_classes: 'btn btn-danger form-control') %>
    </p>   
  </div>
</div>