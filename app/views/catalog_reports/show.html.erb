<p id="notice"><%= notice %></p>
<h2>Detaylar</h2>
<table class="table ">
	<tr>
		<th>Report date:</th>
		<td><span class="badge badge-warning"><%=  @catalog_report.report_date %></span></td>
	</tr>
		<tr>
		<th>Reporter</th>
		<td><span class=""><%= @catalog_report.reporter %></span></td>
	</tr>
	</tr>
		<tr>
		<th>Reporter title</th>
		<td><span class=""><%= @catalog_report.reporter_title %></span></td>
	</tr>

</table>
<p class="p-1"><strong>Notes:</strong></p>
<p class= "p-1"><%= @catalog_report.notes %></p> 

<% if @catalog_report.catalog_analytics.count > 0 %>
	<table class="table table-sm">
		<th>Personel</th>
		<th>New Entry</th>

		<th>Duplicate Entry</th>

		<th>Subject Entry</th>
		<th>Last Update</th>
		<th>Time cretaed</th>
		
		<% @catalog_report.catalog_analytics.each do |catalog_an| %>
			<tr>			
				<td><%= catalog_an.personel%></td>
				<td><%= catalog_an.new_entry%></td>
				<td><%= catalog_an.duplicate_entry%></td>
				<td><%= catalog_an.subject_entry%></td>
				<td><%= catalog_an.updated_at%></td>
				<td><%= catalog_an.created_at%></td>
				
			</tr>
			
		<% end %>
	</table>
<% end %> 
