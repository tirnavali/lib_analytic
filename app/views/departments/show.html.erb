<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @department.name %>
</p>
<p>
  <strong>Reports</strong>
  <%= @department.reference_analytics.name %>
  <% @department.reference_analytics.each do |analytic| %>
    <%=analytic.report_date%>
  <% end %>
</p>
<p>
  <strong>Current Personnel:</strong>
  <% @department.current_employments.each do |employment| %>
    </br><%=employment.personel.name + " - "+ employment.personel.surname%>
  <% end %>
</p>

<%= link_to 'Edit', edit_department_path(@department) %> |
<%= link_to 'Back', departments_path %>
