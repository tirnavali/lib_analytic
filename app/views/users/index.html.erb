<h1>This is users page</h1>

<%= link_to 'New', new_user_registration_path , class: "btn btn-primary"%> 

<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>User Email</th>
      <th>Roles</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.roles %></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
       
        <td><%= link_to 'Edit', edit_user_registration_path(user) %></td>
        <td><%#= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>